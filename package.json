{
  "name": "sapakan",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "start": "ts-node ./src/server.ts",
    "lint": "yarn lint:eslint && yarn lint:prettier && yarn lint:tsc && yarn lint:prisma",
    "lint:eslint": "eslint --ext .ts src",
    "lint:prettier": "prettier --check 'src/**/*.ts'",
    "lint:tsc": "tsc --noEmit",
    "lint:prisma": "dotenv -e .env.test -- prisma validate",
    "fix": "yarn fix:eslint && yarn fix:prettier",
    "fix:eslint": "eslint --fix --ext .ts src",
    "fix:prettier": "prettier --write 'src/**/*.ts'",
    "test": "yarn run test:dbup && yarn run test:reset && yarn run test:run && yarn run test:dbdown",
    "test:dbup": "docker-compose -f docker-compose.test.yml up -d",
    "test:dbdown": "docker-compose -f docker-compose.test.yml down -v",
    "test:reset": "dotenv -e .env.test -- yarn prisma migrate reset --force",
    "test:run": "dotenv -e .env.test -- yarn jest"
  },
  "devDependencies": {
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.4",
    "@types/node": "^18.11.11",
    "@typescript-eslint/eslint-plugin": "^5.46.0",
    "@typescript-eslint/parser": "^5.46.0",
    "dotenv-cli": "^6.0.0",
    "eslint": "^8.29.0",
    "eslint-config-prettier": "^8.5.0",
    "fishery": "^2.2.2",
    "jest": "^29.3.1",
    "node-mocks-http": "^1.12.1",
    "prettier": "^2.8.1",
    "prisma": "^4.7.1",
    "ts-jest": "^29.0.3",
    "typescript": "^4.9.4"
  },
  "dependencies": {
    "@prisma/client": "^4.7.1",
    "@swc/core": "^1.3.21",
    "body-parser": "^1.20.1",
    "express": "^4.18.2",
    "ts-node": "^10.9.1"
  }
}
